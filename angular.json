{
  "version": 2,
  "projects": {
    "admin": "apps/admin",
    "admin-e2e": "apps/admin-e2e",
    "api": "apps/api",
    "auth": {
      "projectType": "library",
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "prefix": "skooltrak",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/auth"
          ],
          "options": {
            "jestConfig": "libs/auth/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/auth/**/*.ts",
              "libs/auth/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "host": "apps/host",
    "host-e2e": "apps/host-e2e",
    "messaging": {
      "projectType": "library",
      "root": "libs/messaging",
      "sourceRoot": "libs/messaging/src",
      "prefix": "skooltrak",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/messaging"
          ],
          "options": {
            "jestConfig": "libs/messaging/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/messaging/**/*.ts",
              "libs/messaging/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/models/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/models"
          ],
          "options": {
            "jestConfig": "libs/models/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "state": {
      "projectType": "library",
      "root": "libs/state",
      "sourceRoot": "libs/state/src",
      "prefix": "skooltrak",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/libs/state"
          ],
          "options": {
            "jestConfig": "libs/state/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/state/**/*.ts",
              "libs/state/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "student": "apps/student",
    "student-e2e": "apps/student-e2e",
    "teacher": "apps/teacher",
    "teacher-e2e": "apps/teacher-e2e",
    "ui": "libs/ui",
    "utils": "libs/utils"
  }
}
