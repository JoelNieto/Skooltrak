{
  "name": "skooltrak-app",
  "version": "0.0.15",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/skooltrak-app/stats.json",
    "test": "ng test skooltrak-app --code-coverage --sourceMap=true",
    "explore": "source-map-explorer dist/skooltrak-app/**/*.js",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "publish-components": "ng build custom-components --configuration production && cd dist/custom-components && npm pack && npm publish",
    "registry": "npm config set registry https://registry.npmjs.org/",
    "build-prod": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production",
    "build-demo": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration demo",
    "test-ci": "ng test skooltrak-app --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN --non-interactive",
    "deploy-moises": "firebase deploy --only hosting:moises",
    "clear": "rm -rf node_modules && rm package-lock.json && npm cache clear --force",
    "start:electron": "ng build --base-href ./ && electron ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "private": true,
  "dependencies": {
