  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test skooltrak-app --sourceMap=true --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish-components": "ng build custom-components --prod && cd dist/custom-components && npm pack",
    "build-prod": "ng build --prod",
    "test-ci": "ng test skooltrak-app --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN --non-interactive",
    "deploy-moises": "firebase deploy --only hosting:moises",
    "compodoc": "npx compodoc -p tsconfig.json --theme stripe"
  },
