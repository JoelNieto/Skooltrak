{
  "name": "skooltrak-app",
  "version": "0.0.15",
  "main": "main.js",
  "scripts": {
    "ng": "nx",
    "start": "nx serve --hmr",
    "build": "nx build",
    "build:stats": "nx build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/skooltrak-app/stats.json",
    "test": "nx test skooltrak-app --code-coverage --watch=false --sourceMap=true --browsers=ChromeHeadless",
    "explore": "source-map-explorer dist/skooltrak-app/**/*.js",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "e2e": "nx e2e",
    "registry": "npm config set registry https://registry.npmjs.org/",
    "build-prod": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration production",
    "build-demo": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration demo",
    "test-ci": "nx test skooltrak-app --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "e2e-ci": "nx e2e --protractor-config=e2e/protractor-ci.conf.js",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN --non-interactive",
    "deploy-moises": "firebase deploy --only hosting:moises",
    "clear": "rm -rf node_modules && rm package-lock.json && npm cache clear --force",
    "start:electron": "nx build --base-href ./ && electron .",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "private": true,
